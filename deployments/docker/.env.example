# V Panel Docker Environment Variables
# Copy this file to .env and modify as needed

# Application version
VERSION=latest

# Server configuration
# 留空则自动生成随机端口 (10000-65000)
V_SERVER_PORT=
V_SERVER_MODE=release

# Authentication
V_JWT_SECRET=your-secure-jwt-secret-change-me
V_ADMIN_USER=admin
V_ADMIN_PASS=your-secure-admin-password

# Logging
V_LOG_LEVEL=info
V_LOG_FORMAT=json

# Timezone
TZ=UTC

# Build information (set during CI/CD)
BUILD_TIME=
GIT_COMMIT=
